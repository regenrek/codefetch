{
  "name": "codefetch-monorepo",
  "version": "1.5.1",
  "private": true,
  "description": "Codefetch monorepo",
  "repository": "regenrek/codefetch",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm -C packages/sdk build && pnpm -C packages/cli build && pnpm -C packages/mcp build",
    "test": "pnpm -r test",
    "cli": "pnpm --filter codefetch run cli",
    "lint": "pnpm -r lint",
    "release:cli": "tsx scripts/release-fix-workspace.ts",
    "release:legacy": "tsx scripts/release.ts",
    "release:sdk": "cd packages/sdk && tsx ../../scripts/release.ts",
    "release:all": "pnpm release:sdk && pnpm release:cli",
    "knip": "knip"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-config-unjs": "^0.4.2",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "3.2.0",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.14.4",
  "dependencies": {
    "knip": "^5.62.0"
  }
}
